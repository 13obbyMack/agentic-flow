# =====================================================
# CORE API KEYS
# =====================================================

# Anthropic API Key (Primary LLM Provider)
ANTHROPIC_API_KEY=sk-ant-...

# OpenRouter API Key (Multi-Model Gateway)
# Get your key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=sk-or-...

# OpenAI API Key (Optional - for GPT models)
# Get your key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-...

# Google API Key (Optional - for Gemini models)
GOOGLE_API_KEY=...

# =====================================================
# ROUTER CONFIGURATION (Optional)
# =====================================================

# Default provider for multi-model router
# Options: anthropic, openai, openrouter, ollama, litellm, onnx
ROUTER_DEFAULT_PROVIDER=anthropic

# Routing mode
# Options: manual, cost-optimized, performance-optimized, rule-based
ROUTER_MODE=cost-optimized

# Ollama base URL (for local models)
OLLAMA_BASE_URL=http://localhost:11434

# LiteLLM proxy URL (if using LiteLLM)
LITELLM_PROXY_URL=http://localhost:8000

# =====================================================
# ONNX RUNTIME CONFIGURATION (Local Inference)
# =====================================================

# ONNX model to use (HuggingFace model ID)
# Examples: Xenova/Phi-3-mini-4k-instruct, Xenova/Phi-3-medium-128k-instruct
ONNX_MODEL_ID=Xenova/Phi-3-mini-4k-instruct

# Execution providers (comma-separated)
# Options: cpu, cuda, dml (DirectML for Windows), webgpu
ONNX_EXECUTION_PROVIDERS=cpu

# Enable GPU acceleration (requires CUDA or DirectML)
ONNX_GPU_ACCELERATION=false

# Max tokens for ONNX inference
ONNX_MAX_TOKENS=512

# =====================================================
# PARALLEL MODE CONFIGURATION
# =====================================================

TOPIC="pricing page split"
DIFF="feat: add new pricing component"
DATASET="user engagement metrics"

# =====================================================
# E2B SANDBOX CONFIGURATION
# =====================================================

# E2B API Key (for cloud code execution)
E2B_API_KEY=e2b_...
E2B_ACCESS_TOKEN=sk_e2b_...

# =====================================================
# SUPABASE CONFIGURATION
# =====================================================

SUPABASE_ACCESS_TOKEN=sbp_...
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=...
SUPABASE_PROJECT_ID=your-project-id

# =====================================================
# FLOW NEXUS CONFIGURATION (Optional)
# =====================================================

FLOW_NEXUS_USER=your-email@example.com
FLOW_NEXUS_PASSWORD=your-password
FLOW_NEXUS_SESSION=...

# =====================================================
# TOKEN ALLOCATION (Optional - for genesis setup)
# =====================================================

TOTAL_RUV_SUPPLY=10000000000
ECOSYSTEM_RESERVE=2500000000
